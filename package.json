    "@reduxjs/toolkit": "^1.9.5",
    "axios": "^1.4.0",
    "date-fns": "^2.30.0",
    "openai": "^4.9.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.1.1",
    "react-router-dom": "^6.12.0",
    "socket.io-client": "^4.7.2",
    "uuid": "^9.0.0",
    "yjs": "^13.6.23"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.0.0",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.5.0",
    "@types/node": "^20.4.2",
    "@types/react": "^18.2.14",
    "@types/react-dom": "^18.2.7",
    "@types/react-router-dom": "^6.4.5",
    "@types/uuid": "^9.0.2",
    "@vitejs/plugin-react": "^4.0.0",
    "@vitest/ui": "^0.30.0",
    "@vitejs/plugin-env-compatible": "^1.3.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.49.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-jest": "^29.2.2",
    "eslint-plugin-jsx-a11y": "^7.8.8",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.9.0",
    "eslint-plugin-testing-library": "^5.11.4",
    "husky": "^8.0.5",
    "lint-staged": "^14.0.5",
    "prettier": "^3.0.0",
    "typescript": "^5.1.6",
    "vite": "^4.4.9",
    "vitest": "^0.30.0"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,css,scss}": [
      "prettier --write"
    ]
  },
  "engines": {
    "node": ">=18",
    "npm": ">=9"
  },
  "browserslist": [
    "defaults",
    "not IE 11",
    "maintained node versions"
  ]
}