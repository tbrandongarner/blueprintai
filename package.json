  },
  "bugs": {
    "url": "https://github.com/your-org/blueprint-ai/issues"
  },
  "homepage": "https://github.com/your-org/blueprint-ai#readme",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext .ts,.tsx",
    "format": "prettier --write .",
    "prepare": "husky install",
    "test": "vitest run --coverage",
