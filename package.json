{
  "name": "blueprint-ai",
  "version": "1.0.0",
  "private": true,
  "description": "BluePrint AI ? design, generate, and manage AI-powered blueprints through an intuitive React interface",
  "author": "Your Name <youremail@example.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
