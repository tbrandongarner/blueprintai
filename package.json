    "date-fns": "^2.30.0",
    "openai": "^4.9.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.1.1",
    "react-router-dom": "^6.12.0",
    "socket.io-client": "^4.7.2",
    "uuid": "^9.0.0",
    "yjs": "^13.6.23"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^4.0.0",
    "vite": "^4.4.9"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,css,scss}": [
      "prettier --write"
    ]
  },
  "engines": {
    "node": ">=18",
    "npm": ">=9"
  },
  "browserslist": [
    "defaults",
    "not IE 11",
