  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/blueprint-ai.git"
  },
  "bugs": {
    "url": "https://github.com/your-org/blueprint-ai/issues"
  },
  "homepage": "https://github.com/your-org/blueprint-ai#readme",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
